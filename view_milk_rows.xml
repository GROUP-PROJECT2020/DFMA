<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/scrollView1"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:id="@+id/Layout"
        android:layout_width="match_parent"
        android:layout_height="210dp"
        android:layout_marginTop="5dp"
        android:orientation="vertical"
        android:padding="15dp">

        <ImageView
            android:id="@+id/milk_icon"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="50dp"
            android:layout_marginLeft="10dp"
            android:src="@drawable/drawable_milk_1" />

        <ImageView
            android:id="@+id/bar_icon_1"
            android:layout_width="10dp"
            android:layout_height="200dp"
            android:layout_marginLeft="20dp"
            android:layout_toRightOf="@+id/milk_icon"
            android:src="@drawable/barblue" />

        <TextView
            android:id="@+id/tv_view"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/milk_icon"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="5dp"
            android:text="@string/Retrieved"
            android:textSize="15dp"
            android:textStyle="italic" />

        <TextView
            android:id="@+id/tv_tag"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/milk_icon"
            android:text="@string/tag"
            android:textSize="15dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tag"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@+id/tv_tag"
            android:text="@string/tag"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/tv_farmer_name"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tag"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/milk_icon"
            android:text="@string/farmer_name"
            android:textSize="15dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/farmer_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tag"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@+id/tv_farmer_name"
            android:text="@string/farmer_name"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/tv_morning"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/farmer_name"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/milk_icon"
            android:text="@string/morning"
            android:textSize="15dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/morning"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/farmer_name"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@+id/tv_morning"
            android:text="@string/morning"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/tv_mid_morning"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/morning"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/milk_icon"
            android:text="@string/mid_morning"
            android:textSize="15dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/mid_morning"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/morning"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@+id/tv_mid_morning"
            android:text="@string/mid_morning"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/tv_evening"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/mid_morning"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/milk_icon"
            android:text="@string/evening"
            android:textSize="15dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/evening"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/mid_morning"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@+id/tv_evening"
            android:text="@string/evening"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/tv_domestic"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/evening"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/milk_icon"
            android:text="@string/domestic"
            android:textSize="15dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/domestic"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/evening"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@+id/tv_domestic"
            android:text="@string/domestic"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/tv_sale"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/domestic"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/milk_icon"
            android:text="@string/sale"
            android:textSize="15dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/sale"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/domestic"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@+id/tv_sale"
            android:text="@string/sale"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/tv_buyer"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/sale"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/milk_icon"
            android:text="@string/buyer"
            android:textSize="15dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/buyer"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/sale"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@+id/tv_buyer"
            android:text="@string/buyer"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/tv_date_today"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/buyer"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/milk_icon"
            android:text="@string/date_today"
            android:textStyle="bold"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/date_today"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/buyer"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@+id/tv_date_today"
            android:text="@string/date_today"
            android:textSize="15dp" />


        <ImageView
            android:id="@+id/bar_icon_3"
            android:layout_width="10dp"
            android:layout_height="200dp"
            android:layout_marginLeft="300dp"
            android:layout_toRightOf="@+id/cow_icon"
            android:src="@drawable/barblue" />
    </RelativeLayout>
</androidx.cardview.widget.CardView>